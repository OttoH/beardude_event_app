{
  "name": "beardude_event_react",
  "version": "1.0.0",
  "description": "A bicycle race event manager",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e xyz --config nodemon.dev.json",
    "clean": "rm -rf dist",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js;npm run build:server",
    "build:server": "NODE_ENV=production babel server.js --out-dir dist/",
    "rebuild": "npm run clean && npm run build",
    "server": "NODE_ENV=production node ./dist/server.js",
    "lint": "standard ./src/*.js webpack.config.*.js",
    "test": "JEST_JUNIT_OUTPUT=./test_result/jest.xml jest --config ./jest.json --coverage --coverageDirectory=./test_result/coverage/"
  },
  "keywords": [
    "beardude",
    "bicycle race",
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:azaitw/beardude_event.git"
  },
  "author": "azai",
  "license": "",
  "standard": {
    "ignore": [
      "config/**/*",
      "dist/"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.15.3",
    "express-http-proxy": "^1.0.6",
    "query-string": "^4.3.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^2.0.3",
    "url-search-params-polyfill": "^1.3.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "*",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.7",
    "enzyme": "^2.9.1",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "*",
    "html-minifier": "3.5.2",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "istanbul": "0.4.5",
    "jest": "^20.0.4",
    "jest-junit": "^2.1.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.5",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "next",
    "standard": "^10.0.2",
    "style-loader": "^0.18.2",
    "url-loader": "0.5.9",
    "webpack": "^2.6.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-cleanup-plugin": "^0.5.1",
